#!/bin/bash
mkdir -p static/CACHE/models
django-admin.py graph_models -a -g -X AttachmentImageRaw,TimestampModelMixin,PermissionsMixin,AbstractBaseUser,AbstractUser -o static/CACHE/models/models.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin accounts -o static/CACHE/models/accounts.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin article -o static/CACHE/models/article.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin attachment -o static/CACHE/models/attachment.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin blackhole -o static/CACHE/models/blackhole.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin blog -o static/CACHE/models/blog.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin comments -o static/CACHE/models/comments.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin desktops -o static/CACHE/models/desktops.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin forum -o static/CACHE/models/forum.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin news -o static/CACHE/models/news.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin notes -o static/CACHE/models/notes.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin notifications -o static/CACHE/models/notifications.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin polls -o static/CACHE/models/polls.png
django-admin.py graph_models -X AttachmentImageRaw,TimestampModelMixin wiki -o static/CACHE/models/wiki.png
